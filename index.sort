@Pass{
FS_PREC(mediump,float)
#ifdef VS
  attribute vec3 position;
  uniform mat4 _matPVM;
  void main(){
    gl_Position = _matPVM * vec4(position,1);
  }
#endif

#ifdef FS
@{ type:"color",default:"orange"}
uniform vec4 color;

@{range:"0,10",default:"0"}
uniform float a;
 void main() {
   gl_FragColor = color;
   gl_FragColor.rgb*= a;
 }
#endif
}
